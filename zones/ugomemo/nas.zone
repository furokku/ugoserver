;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.nas.nintendowifi.net. nas.nintendowifi.net. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
	IN	NS	ns.nas.nintendowifi.net.
ns.nas.nintendowifi.net.	IN	A	a.b.c.d

nas.nintendowifi.net.	IN	A	a.b.c.d
