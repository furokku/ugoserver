;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.flipnote.hatena.com. flipnote.hatena.com. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
@	IN	NS	ns.flipnote.hatena.com.
ns.flipnote.hatena.com.	IN	A	a.b.c.d

flipnote.hatena.com.	IN	A	a.b.c.d
